library("ggplot2")
data = read.table("project-issue-comment-developer.data", header = T)
m <- ggplot(data, aes(x = project))
m + geom_histogram()
m <- ggplot(data, aes(x = project, y = issue))
m + geom_bar()
m <- ggplot(data, aes(x = project, factor(issue)))
m + geom_bar()
m <- ggplot(data, aes(x = project))
m + geom_bar()
m + geom_bar()
